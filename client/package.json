{
  "name": "@dungeon-delvers/client",
  "version": "0.0.1",
  "repository": "https://github.com/dungeon_delvers/Dungeon_Delvers",
  "description": "The Front End 3D Client for Dungeon Delvers powered by BabylonJS",
  "homepage": "./",
  "assetsPath": "public/assets",
  "directories": {
    "buildResources": "dist",
    "app": "dist"
  },
  "scripts": {
    "build": "node ./build.mjs",
    "clean": "rm -rf .parcel-cache dist node_modules",
    "deploy-demo": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "start:dev": "parcel ./public/index.html --port 3000",
    "start": "parcel build public/index.html --dist-dir dist && serve dist",
    "test": "jest"
  },
  "keywords": [
    "BabylonJS",
    "Browser",
    "Game",
    "Javascript",
    "Typescript",
    "3D"
  ],
  "author": "Shawn Mealey",
  "license": "ISC",
  "dependencies": {
    "@babylonjs/core": "^7.34.2",
    "@babylonjs/gui": "^7.34.2",
    "@babylonjs/inspector": "^7.34.2",
    "@babylonjs/loaders": "^7.34.2",
    "@babylonjs/materials": "^7.34.2",
    "@dungeon-delvers/config": "0.0.12",
    "@parcel/config-default": "^2.9.2",
    "alea": "^1.0.1",
    "delaunay-triangulate": "^1.1.6",
    "dotenv": "^16.4.5",
    "earcut": "^3.0.0",
    "parcel": "^2.12.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "serve": "^14.2.4",
    "simplex-noise": "^4.0.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@parcel/types": "^2.9.2",
    "@types/earcut": "^2.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.2",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "jest": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.7.2",
    "vm-browserify": "^1.1.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
